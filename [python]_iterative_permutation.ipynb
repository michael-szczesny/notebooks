      "source": [
        "def permute(l):\n",
        "    p = [l[:1]]\n",
        "    for i, _ in enumerate(l[1:], 2):\n",
        "        p = [k[:j]+l[i-1:i]+k[j:] for k in p for j in range(i)]\n",
        "    return p\n",
        "permute([1,2,3,4])"
      ],
      "execution_count": 1,
